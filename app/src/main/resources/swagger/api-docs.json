{
  "openapi" : "3.0.3",
  "info" : {
    "title" : "selc-onboarding",
    "description" : "Self Care OnBoarding API documentation",
    "version" : "0.0.1-SNAPSHOT"
  },
  "servers" : [ {
    "url" : "http://localhost:80",
    "description" : "Inferred Url"
  } ],
  "tags" : [ {
    "name" : "institutions",
    "description" : "Institution operations"
  }, {
    "name" : "product",
    "description" : "Product operations"
  } ],
  "paths" : {
    "/institutions" : {
      "get" : {
        "tags" : [ "institutions" ],
        "summary" : "getInstitutions",
        "description" : "The service retrieves all the onboarded institutions",
        "operationId" : "getInstitutionsUsingGET",
        "responses" : {
          "200" : {
            "description" : "OK",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "array",
                  "items" : {
                    "$ref" : "#/components/schemas/InstitutionResource"
                  }
                }
              }
            }
          },
          "401" : {
            "description" : "Unauthorized"
          },
          "403" : {
            "description" : "Forbidden"
          },
          "404" : {
            "description" : "Not Found"
          }
        },
        "security" : [ {
          "bearerAuth" : [ "global" ]
        } ]
      }
    },
    "/institutions/{institutionId}/data" : {
      "get" : {
        "tags" : [ "institutions" ],
        "summary" : "getInstitutionData",
        "description" : "${swagger.onboarding.institutions.api.manager}",
        "operationId" : "getInstitutionDataUsingGET",
        "parameters" : [ {
          "name" : "institutionId",
          "in" : "path",
          "description" : "Institution's unique identifier (i.e.: IPA code)",
          "required" : true,
          "style" : "simple",
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "OK",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/InstitutionResource"
                }
              }
            }
          },
          "401" : {
            "description" : "Unauthorized"
          },
          "403" : {
            "description" : "Forbidden"
          },
          "404" : {
            "description" : "Not Found"
          }
        },
        "security" : [ {
          "bearerAuth" : [ "global" ]
        } ]
      }
    },
    "/institutions/{institutionId}/products/{productId}/onboarded-institution-info" : {
      "get" : {
        "tags" : [ "institutions" ],
        "summary" : "getInstitutionOnboardingInfo",
        "description" : "The service retrieves the institutions Manager and institution informations",
        "operationId" : "getInstitutionOnboardingInfoUsingGET",
        "parameters" : [ {
          "name" : "institutionId",
          "in" : "path",
          "description" : "Institution's unique identifier (i.e.: IPA code)",
          "required" : true,
          "style" : "simple",
          "schema" : {
            "type" : "string"
          }
        }, {
          "name" : "productId",
          "in" : "path",
          "description" : "Product's unique identifier",
          "required" : true,
          "style" : "simple",
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "OK",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/InstitutionOnboardingInfoResource"
                }
              }
            }
          },
          "401" : {
            "description" : "Unauthorized"
          },
          "403" : {
            "description" : "Forbidden"
          },
          "404" : {
            "description" : "Not Found"
          }
        },
        "security" : [ {
          "bearerAuth" : [ "global" ]
        } ]
      }
    },
    "/institutions/{institutionId}/products/{productId}/onboarding" : {
      "post" : {
        "tags" : [ "institutions" ],
        "summary" : "onboarding",
        "description" : "The service allows the onboarding of Users to an Institution",
        "operationId" : "onboardingUsingPOST",
        "parameters" : [ {
          "name" : "institutionId",
          "in" : "path",
          "description" : "Institution's unique identifier (i.e.: IPA code)",
          "required" : true,
          "style" : "simple",
          "schema" : {
            "type" : "string"
          }
        }, {
          "name" : "productId",
          "in" : "path",
          "description" : "Product's unique identifier",
          "required" : true,
          "style" : "simple",
          "schema" : {
            "type" : "string"
          }
        } ],
        "requestBody" : {
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/OnboardingDto"
              }
            }
          }
        },
        "responses" : {
          "201" : {
            "description" : "Created"
          },
          "401" : {
            "description" : "Unauthorized"
          },
          "403" : {
            "description" : "Forbidden"
          },
          "404" : {
            "description" : "Not Found"
          }
        },
        "security" : [ {
          "bearerAuth" : [ "global" ]
        } ]
      }
    },
    "/product/{id}" : {
      "get" : {
        "tags" : [ "product" ],
        "summary" : "getProduct",
        "description" : "${swagger.onboarding.operation.getProduct}",
        "operationId" : "getProductUsingGET",
        "parameters" : [ {
          "name" : "id",
          "in" : "path",
          "description" : "Product id",
          "required" : true,
          "style" : "simple",
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "OK",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ProductResource"
                }
              }
            }
          },
          "401" : {
            "description" : "Unauthorized"
          },
          "403" : {
            "description" : "Forbidden"
          },
          "404" : {
            "description" : "Not Found"
          }
        },
        "security" : [ {
          "bearerAuth" : [ "global" ]
        } ]
      }
    }
  },
  "components" : {
    "schemas" : {
      "BillingDataDto" : {
        "title" : "BillingDataDto",
        "required" : [ "businessName", "digitalAddress", "recipientCode", "registeredOffice", "taxCode", "vatNumber" ],
        "type" : "object",
        "properties" : {
          "businessName" : {
            "type" : "string",
            "description" : "Institution's legal name"
          },
          "digitalAddress" : {
            "type" : "string",
            "description" : "Institution's digitalAddress"
          },
          "publicServices": {
            "type": "boolean",
            "description": "Institution's service type",
            "example": false
          },
          "recipientCode" : {
            "type" : "string",
            "description" : "Billing recipient code"
          },
          "registeredOffice" : {
            "type" : "string",
            "description" : "Institution's physical address"
          },
          "taxCode" : {
            "type" : "string",
            "description" : "Institution's taxCode"
          },
          "vatNumber" : {
            "type" : "string",
            "description" : "Institution's VAT number"
          }
        }
      },
      "InstitutionData" : {
        "title" : "InstitutionData",
        "type" : "object",
        "properties" : {
          "billingData" : {
            "description" : "Institution's billing informations",
            "$ref" : "#/components/schemas/BillingDataDto"
          },
          "institutionType" : {
            "type" : "string",
            "description" : "Institution's type",
            "enum" : [ "GSP", "PA", "PT", "SCP" ]
          },
          "origin" : {
            "type" : "string"
          }
        }
      },
      "InstitutionOnboardingInfoResource" : {
        "title" : "InstitutionOnboardingInfoResource",
        "type" : "object",
        "properties" : {
          "institution" : {
            "description" : "Institution's billing and type informations",
            "$ref" : "#/components/schemas/InstitutionData"
          },
          "manager" : {
            "description" : "Institution Manager's informations",
            "$ref" : "#/components/schemas/UserResource"
          }
        }
      },
      "InstitutionResource" : {
        "title" : "InstitutionResource",
        "required" : [ "address", "description", "digitalAddress", "id", "institutionId", "origin", "taxCode", "zipCode" ],
        "type" : "object",
        "properties" : {
          "address" : {
            "type" : "string",
            "description" : "Institution's physical address"
          },
          "description" : {
            "type" : "string",
            "description" : "Institution's legal name"
          },
          "digitalAddress" : {
            "type" : "string",
            "description" : "Institution's digitalAddress"
          },
          "id" : {
            "type" : "string",
            "description" : "Institution's unique identifier (i.e.: IPA code)"
          },
          "institutionId" : {
            "type" : "string",
            "description" : "Institution's unique Id"
          },
          "origin" : {
            "type" : "string",
            "description" : "Institution data origin"
          },
          "taxCode" : {
            "type" : "string",
            "description" : "Institution's taxCode"
          },
          "zipCode" : {
            "type" : "string",
            "description" : "Institution's zipCode"
          }
        }
      },
      "OnboardingDto" : {
        "title" : "OnboardingDto",
        "required" : [ "billingData", "institutionType", "users" ],
        "type" : "object",
        "properties" : {
          "billingData" : {
            "description" : "Institution's billing informations",
            "$ref" : "#/components/schemas/BillingDataDto"
          },
          "institutionType" : {
            "type" : "string",
            "description" : "Institution's type",
            "enum" : [ "GSP", "PA", "PT", "SCP" ]
          },
          "origin" : {
            "type" : "string",
            "description" : "Institution data origin"
          },
          "pricingPlan" : {
            "type" : "string",
            "description" : "Product's pricing plan"
          },
          "users" : {
            "type" : "array",
            "description" : "List of onboarding users",
            "items" : {
              "$ref" : "#/components/schemas/UserDto"
            }
          }
        }
      },
      "ProductResource" : {
        "title" : "ProductResource",
        "required" : [ "id", "title" ],
        "type" : "object",
        "properties" : {
          "id" : {
            "type" : "string",
            "description" : "Product id"
          },
          "parent" : {
            "type" : "string",
            "description" : "Product's parent's Id"
          },
          "title" : {
            "type" : "string",
            "description" : "Product's title"
          }
        }
      },
      "UserDto" : {
        "title" : "UserDto",
        "required" : [ "email", "name", "role", "surname", "taxCode" ],
        "type" : "object",
        "properties" : {
          "email" : {
            "type" : "string",
            "description" : "User's email"
          },
          "name" : {
            "type" : "string",
            "description" : "User's name"
          },
          "role" : {
            "type" : "string",
            "description" : "User's role",
            "enum" : [ "DELEGATE", "MANAGER", "OPERATOR", "SUB_DELEGATE" ]
          },
          "surname" : {
            "type" : "string",
            "description" : "User's surname"
          },
          "taxCode" : {
            "type" : "string",
            "description" : "User's fiscal code"
          }
        }
      },
      "UserResource" : {
        "title" : "UserResource",
        "required" : [ "certified", "email", "id", "institutionId", "name", "role", "status", "surname", "taxCode" ],
        "type" : "object",
        "properties" : {
          "certified" : {
            "type" : "boolean",
            "description" : "${swagger.onboarding.user.model.certified}",
            "example" : false
          },
          "email" : {
            "type" : "string",
            "description" : "User's email"
          },
          "id" : {
            "type" : "string",
            "description" : "${swagger.onboarding.user.model.id}"
          },
          "institutionId" : {
            "type" : "string",
            "description" : "${swagger.onboarding.user.model.institutionId}"
          },
          "name" : {
            "type" : "string",
            "description" : "User's name"
          },
          "role" : {
            "type" : "string",
            "description" : "User's role",
            "enum" : [ "DELEGATE", "MANAGER", "OPERATOR", "SUB_DELEGATE" ]
          },
          "status" : {
            "type" : "string",
            "description" : "${swagger.onboarding.user.model.status}"
          },
          "surname" : {
            "type" : "string",
            "description" : "User's surname"
          },
          "taxCode" : {
            "type" : "string",
            "description" : "User's fiscal code"
          }
        }
      }
    },
    "securitySchemes" : {
      "bearerAuth" : {
        "type" : "http",
        "description" : "A bearer token in the format of a JWS and conformed to the specifications included in [RFC8725](https://tools.ietf.org/html/RFC8725)",
        "scheme" : "bearer",
        "bearerFormat" : "JWT"
      }
    }
  }
}