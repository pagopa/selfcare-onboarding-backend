{
  "openapi": "3.0.3",
  "info": {
    "title": "selc-onboarding",
    "description": "Self Care OnBoarding API documentation",
    "version": "0.0.1-SNAPSHOT"
  },
  "servers": [
    {
      "url": "http://localhost:80",
      "description": "Inferred Url"
    }
  ],
  "tags": [
    {
      "name": "institutions",
      "description": "Institution operations"
    },
    {
      "name": "product",
      "description": "Product operations"
    }
  ],
  "paths": {
    "/institutions": {
      "get": {
        "tags": [
          "institutions"
        ],
        "summary": "getInstitutions",
        "description": "${swagger.dashboard.institutions.api.getInstitutions}",
        "operationId": "getInstitutionsUsingGET",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/InstitutionResource"
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "security": [
          {
            "bearerAuth": [
              "global"
            ]
          }
        ]
      }
    },
    "/institutions/{institutionId}/data": {
      "get": {
        "tags": [
          "institutions"
        ],
        "summary": "getInstitutionData",
        "description": "${swagger.onboarding.institutions.api.manager}",
        "operationId": "getInstitutionDataUsingGET",
        "parameters": [
          {
            "name": "institutionId",
            "in": "path",
            "description": "Institution's unique identifier (i.e.: IPA code)",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InstitutionData"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "security": [
          {
            "bearerAuth": [
              "global"
            ]
          }
        ]
      }
    },
    "/institutions/{institutionId}/products/{productId}/onboarded-institution-info": {
      "get": {
        "tags": [
          "institutions"
        ],
        "summary": "getInstitutionOnboardingInfo",
        "description": "The service retrieves the institutions Manager and institution informations",
        "operationId": "getInstitutionOnboardingInfoUsingGET",
        "parameters": [
          {
            "name": "institutionId",
            "in": "path",
            "description": "Institution's unique identifier (i.e.: IPA code)",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "productId",
            "in": "path",
            "description": "Product's unique identifier",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InstitutionOnboardingInfoResource"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "security": [
          {
            "bearerAuth": [
              "global"
            ]
          }
        ]
      }
    },
    "/institutions/{institutionId}/products/{productId}/onboarding": {
      "post": {
        "tags": [
          "institutions"
        ],
        "summary": "onboarding",
        "description": "The service allows the onboarding of Users to an Institution",
        "operationId": "onboardingUsingPOST",
        "parameters": [
          {
            "name": "institutionId",
            "in": "path",
            "description": "Institution's unique identifier (i.e.: IPA code)",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "productId",
            "in": "path",
            "description": "Product's unique identifier",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/OnboardingDto"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/OnboardingResponse"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "security": [
          {
            "bearerAuth": [
              "global"
            ]
          }
        ]
      }
    },
    "/product/{id}": {
      "get": {
        "tags": [
          "product"
        ],
        "summary": "getProduct",
        "description": "${swagger.onboarding.operation.getProduct}",
        "operationId": "getProductUsingGET",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "Product id",
            "required": true,
            "style": "simple",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ProductResource"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "security": [
          {
            "bearerAuth": [
              "global"
            ]
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "BillingDataDto": {
        "title": "BillingDataDto",
        "type": "object",
        "properties": {
          "businessName": {
            "type": "string",
            "description": "${}"
          },
          "digitalAddress": {
            "type": "string"
          },
          "publicService": {
            "type": "boolean"
          },
          "recipientCode": {
            "type": "string"
          },
          "registeredOffice": {
            "type": "string"
          },
          "taxCode": {
            "type": "string"
          },
          "vatNumber": {
            "type": "string"
          }
        }
      },
      "InstitutionData": {
        "title": "InstitutionData",
        "type": "object",
        "properties": {
          "billingData": {
            "description": "Institution's billing informations",
            "$ref": "#/components/schemas/BillingDataDto"
          },
          "organizationType": {
            "type": "string",
            "description": "Institution's type",
            "enum": [
              "GSP",
              "PA",
              "PT",
              "SCP"
            ]
          }
        }
      },
      "InstitutionOnboardingInfoResource": {
        "title": "InstitutionOnboardingInfoResource",
        "type": "object",
        "properties": {
          "institution": {
            "description": "Institution's billing and type informations",
            "$ref": "#/components/schemas/InstitutionData"
          },
          "manager": {
            "description": "Institution Manager's informations",
            "$ref": "#/components/schemas/UserDto"
          }
        }
      },
      "InstitutionResource": {
        "title": "InstitutionResource",
        "required": [
          "address",
          "description",
          "digitalAddress",
          "institutionId",
          "taxCode",
          "zipCode"
        ],
        "type": "object",
        "properties": {
          "address": {
            "type": "string",
            "description": "Institution's physical address"
          },
          "description": {
            "type": "string",
            "description": "Institution's legal name"
          },
          "digitalAddress": {
            "type": "string",
            "description": "Institution's digitalAddress"
          },
          "institutionId": {
            "type": "string",
            "description": "Institution's unique Id"
          },
          "taxCode": {
            "type": "string",
            "description": "Institution's taxCode"
          },
          "zipCode": {
            "type": "string",
            "description": "Institution's zipCode"
          }
        }
      },
      "OnboardingDto": {
        "title": "OnboardingDto",
        "required": [
          "organizationType",
          "users"
        ],
        "type": "object",
        "properties": {
          "billingData": {
            "description": "Institution's billing informations",
            "$ref": "#/components/schemas/BillingDataDto"
          },
          "organizationType": {
            "type": "string",
            "description": "Institution's type",
            "enum": [
              "GSP",
              "PA",
              "PT",
              "SCP"
            ]
          },
          "users": {
            "type": "array",
            "description": "List of onboarding users",
            "items": {
              "$ref": "#/components/schemas/UserDto"
            }
          }
        }
      },
      "OnboardingResponse": {
        "title": "OnboardingResponse",
        "type": "object",
        "properties": {
          "document": {
            "type": "string",
            "format": "binary"
          },
          "token": {
            "type": "string"
          }
        }
      },
      "ProductResource": {
        "title": "ProductResource",
        "required": [
          "id",
          "title"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Product id"
          },
          "parent": {
            "type": "string",
            "description": "Product's parent's Id"
          },
          "title": {
            "type": "string",
            "description": "Product's title"
          }
        }
      },
      "UserDto": {
        "title": "UserDto",
        "required": [
          "email",
          "name",
          "role",
          "surname",
          "taxCode"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "User's email"
          },
          "name": {
            "type": "string",
            "description": "User's name"
          },
          "role": {
            "type": "string",
            "description": "User's role",
            "enum": [
              "DELEGATE",
              "MANAGER",
              "OPERATOR",
              "SUB_DELEGATE"
            ]
          },
          "surname": {
            "type": "string",
            "description": "User's surname"
          },
          "taxCode": {
            "type": "string",
            "description": "User's fiscal code"
          }
        }
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "description": "A bearer token in the format of a JWS and conformed to the specifications included in [RFC8725](https://tools.ietf.org/html/RFC8725)",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}